<h1>All Shelters</h1>
<% @shelters.each do |shelter| %>
	<section class="shelter-<%= shelter.id %>">	
		<h3><a href="/shelters/<%= shelter.id %>"><%= shelter.name %></a></h3>
			<%= link_to 'Edit', "/shelters/#{shelter.id}/edit" %> 
			<%= form_tag "/shelters/#{shelter.id}", method: :delete do %>
				  <%= submit_tag 'Delete' %>
			<% end %>
	</section>
<% end %>	
</section>
<a href="/shelters/new">New Shelter</a>
